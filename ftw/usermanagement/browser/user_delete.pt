
<form method="POST" action="./user_delete" i18n:domain="ftw.usermanagement">
    <h1 i18n:translate="label_delete_user">
        Are you sure, that you want to delete the selected user(s)?
    </h1>

    <ul class="userList">
        <li tal:repeat="user view/users_to_delete">
            <span tal:replace="user/fullname" />
        </li>
    </ul>
    <input tal:replace="structure context/@@authenticator/authenticator" /> 
    <input type="submit" name="form.submitted" value="Delete user(s)" i18n:attributes="value"/>

    <input type="submit" name="form.Cancel" value="Cancel" i18n:attributes="value"/>

</form>
